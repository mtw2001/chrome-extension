{
    "manifest_version": 3,
    "name": "Study Mode",
    "description": "This extension can be enabled to block websites from a pre-determined list.",
    "version": "1.0",
    "icons": {
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["webRequest", "storage"],
    "action": {
        "default_popup": "popup.html"
    }
  }