{
    "manifest_version": 3,
    "name": "Study Mode",
    "description": "This extension can be enabled to block websites from a pre-determined list.",
    "version": "1.0",
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": "icon.png"
    },
    "permissions": ["storage", "tabs"],
    "background": {
        "service_worker": "background/background.js"
    }
}